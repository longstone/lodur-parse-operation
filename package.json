    "debug": "~2.2.0",
    "express": "~4.13.0",
    "express-winston": "^2.1.3",
    "jade": "~1.11.0",
    "lodash": "^4.16.4",
    "moment": "^2.10.3",
    "mongodb": "^2.0.35",
    "mongoose": "^4.0.6",
    "node-telegram-bot": "^0.1.9",
    "q": "^1.4.1",
    "request": "^2.58.0",
    "serve-favicon": "~2.3.0",
    "string": "^3.3.0",
    "underscore": "^1.8.3",
    "webpack-node-externals": "^1.5.4",
    "winston": "^1.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
