{
  "name": "lodurparser",
  "version": "0.0.1",
  "private": true,
  "main": "./bin/app.bundle.js",
  "scripts": {
    "start": "node ./bin/app.bundle.js",
    "start-dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.js --colors && node ./bin/app.bundle.js",
    "test": "mochapack --webpack-config webpack.config-test.babel.js \"src/**/*-spec.js\"",
    "test-watch": "mochapack --webpack-config webpack.config-test.babel.js \"src/**/*-spec.js\" --watch",
    "init": "npm install yarn && yarn install",
    "build": "npm run-script test && cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --colors",
    "build-dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.js --progress --colors",
    "build-dev-watch": "cross-env NODE_ENV=development webpack --config ./webpack.config.js --progress --colors --watch",
    "heroku-postbuild": "npm install webpack && webpack -p --config ./webpack.config.js --progress"
  },
  "engines": {
    "node": ">=12.14.1",
    "npm": ">=6.13.4"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "express-winston": "^4.0.3",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "mongodb": "^3.5.8",
    "mongoose": "^5.9.18",
    "node-telegram-bot": "^0.1.9",
    "q": "^1.5.1",
    "request": "^2.88.2",
    "serve-favicon": "~2.5.0",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.19",
    "string": "^3.3.3",
    "underscore": "^1.10.2",
    "winston": "^3.2.1",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "file-loader": "^6.0.0",
    "mocha": "^7.0.0",
    "mochapack": "^2.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-webstorm-debugger-script": "^1.0.1"
  }
}
