{
  "name": "lodurparser",
  "version": "0.0.1",
  "private": true,
  "main": "./bin/app.bundle.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --colors && node ./bin/app.bundle.js",
    "test": "mocha-webpack --webpack-config webpack.config-test.babel.js \"src/**/*-spec.js\"",
    "test-watch": "mocha-webpack --webpack-config webpack.config-test.babel.js \"src/**/*-spec.js\" --watch",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --colors",
    "build-dev": "cross-env NODE_ENV=developement webpack --config ./webpack.config.js --progress --colors",
    "heroku-postbuild": "npm install -g webpack && webpack -p --config ./webpack.config.js --progress"
  },
  "engines": {
    "node": "6.5.0",
    "npm": "4.0.5"
  },
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "body-parser": "^1.16.1",
    "cheerio": "^0.22.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^3.1.4",
    "debug": "~2.6.1",
    "express": "~4.14.1",
    "express-winston": "^2.2.0",
    "jade": "~1.11.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "mongodb": "^2.2.22",
    "mongoose": "^4.8.2",
    "node-telegram-bot": "^0.1.9",
    "q": "^1.4.1",
    "request": "^2.79.0",
    "serve-favicon": "~2.3.2",
    "source-map-support": "^0.4.11",
    "string": "^3.3.3",
    "underscore": "^1.8.3",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.5.4",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0"
  }
}
