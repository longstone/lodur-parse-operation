{
  "name": "lodurparser",
  "version": "0.0.1",
  "private": true,
  "main": "./bin/app.bundle.js",
  "scripts": {
    "start": "node ./bin/app.bundle.js",
    "start-dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.js --colors && node ./bin/app.bundle.js",
    "test": "mocha-webpack --webpack-config webpack.config-test.babel.js \"src/**/*-spec.js\"",
    "test-watch": "mocha-webpack --webpack-config webpack.config-test.babel.js \"src/**/*-spec.js\" --watch",
    "build": "npm run-script test && cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --colors",
    "build-dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.js --progress --colors",
    "heroku-postbuild": "npm install -g webpack && webpack -p --config ./webpack.config.js --progress"
  },
  "engines": {
    "node": "6.5.0",
    "npm": "4.0.5"
  },
  "dependencies": {
    "body-parser": "^1.16.1",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.0.5",
    "debug": "~3.1.0",
    "express": "~4.16.2",
    "express-winston": "^2.2.0",
    "jade": "~1.11.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "mongodb": "^2.2.22",
    "mongoose": "^4.8.2",
    "node-telegram-bot": "^0.1.9",
    "q": "^1.4.1",
    "request": "^2.79.0",
    "serve-favicon": "~2.4.5",
    "source-map": "^0.6.1",
    "source-map-support": "^0.5.0",
    "string": "^3.3.3",
    "underscore": "^1.8.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.22.0",
    "file-loader": "^1.1.5",
    "mocha": "^3.0.0",
    "mocha-webpack": "^0.7.0",
    "webpack": "^2.1.0-beta.15",
    "webpack-node-externals": "^1.6.0"
  }
}
