{
  "name": "lodurparser",
  "version": "0.0.1",
  "private": true,
  "main": "./bin/app.bundle.js",
  "scripts": {
    "start": "node ./bin/app.bundle.js",
    "start-dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.js --colors && node ./bin/app.bundle.js",
    "test": "mocha-webpack --webpack-config webpack.config-test.babel.js \"src/**/*-spec.js\"",
    "test-watch": "mocha-webpack --webpack-config webpack.config-test.babel.js \"src/**/*-spec.js\" --watch",
    "build": "npm run-script test && cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --colors",
    "build-dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.js --progress --colors",
    "heroku-postbuild": "npm install webpack && webpack -p --config ./webpack.config.js --progress"
  },
  "engines": {
    "node": ">=12.14.1",
    "npm": ">=6.13.4"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^6.0.3",
    "debug": "~3.1.0",
    "express": "~4.16.2",
    "express-winston": "^4.0.2",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "mongodb": "^2.2.22",
    "mongoose": "^5.4.2",
    "node-telegram-bot": "^0.1.9",
    "q": "^1.4.1",
    "request": "^2.88.0",
    "serve-favicon": "~2.5.0",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.0",
    "string": "^3.3.3",
    "underscore": "^1.8.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "file-loader": "^2.0.0",
    "mocha": "^7.0.0",
    "mocha-webpack": "^1.1.0",
    "webpack": "^2.1.0-beta.15",
    "webpack-node-externals": "^1.6.0",
    "webpack-webstorm-debugger-script": "^1.0.1"
  }
}
